<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="59.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.DataController">
   <children>
      <HBox layoutX="7.0" layoutY="9.0" prefHeight="40.0" prefWidth="584.0" spacing="15.0">
         <children>
            <Label fx:id="number" text="번호">
               <HBox.margin>
                  <Insets top="13.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="addressname" text="주소록명">
               <HBox.margin>
                  <Insets top="13.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="ispublic" text="공개범위">
               <HBox.margin>
                  <Insets top="13.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="username" text="등록자">
               <HBox.margin>
                  <Insets top="13.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="date" text="등록일">
               <HBox.margin>
                  <Insets top="13.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="deletebtn" mnemonicParsing="false" onAction="#clickDeleteBtn" prefHeight="23.0" prefWidth="49.0" text="삭제">
               <HBox.margin>
                  <Insets top="9.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="updatebtn" mnemonicParsing="false" onAction="#clickUpdateBtn" prefHeight="23.0" prefWidth="49.0" text="수정">
               <HBox.margin>
                  <Insets top="9.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
